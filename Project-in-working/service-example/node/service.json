{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "status": {
      "path": "/status/:code",
      "public": true,
      "settingsType": "workspace"
    },
    "search": {
      "path": "/itglobers/giftcards/_search",
      "public": true,
      "settingsType": "workspace"
    },
    "balance": {
      "path": "/itglobers/giftcards/:id",
      "public": true,
      "settingsType": "workspace"
    },
    "transactions": {
      "path": "/itglobers/giftcards/:id/transactions",
      "public": true,
      "settingsType": "workspace"
    },
    "create": {
      "path": "/itglobers/product",
      "public": true
    }
  }
}
