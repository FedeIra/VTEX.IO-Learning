{
  "name": "fedeira-app",
  "vendor": "itglobers",
  "version": "0.2.21",
  "title": "Service Example Fedeira",
  "description": "Service Example Fedeira for giftcard and transactions",
  "mustUpdateAt": "2018-01-04",
  "categories": [],
  "dependencies": {
    "vtex.most-amazing-service-ever": "0.x"
  },
  "builders": {
    "node": "6.x",
    "docs": "0.x",
    "configuration": "0.x",
    "vtex.most-amazing-service-ever": "0.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "httpstat.us",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/catalog/pvt/product/*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "OMSViewer"
    },
    {
      "name": "POWER_USER_DS"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
